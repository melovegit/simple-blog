<h1><%= @post.title %></h1>

<p>Submitted <%= time_ago_in_words(@post.created_at) %> ago</p>


<p><%= @post.body %></p>

<p><%= current_user.email %></p>

  <% if user_signed_in? %>
<%= link_to 'Edit', edit_post_path(@post) %>

<%= link_to 'Delete', post_path(@post), method: :delete %>

<% end %>


